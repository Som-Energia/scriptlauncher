<script>
    
        $("#test-link").click(function(){
            if ($("#test").css("display")=="none"){
                $("#test").show("fast");
            }
            else {
                $("#test").hide("fast");
            }
        }
        );
    
        $("#workingDirTests-link").click(function(){
            if ($("#workingDirTests").css("display")=="none"){
                $("#workingDirTests").show("fast");
            }
            else {
                $("#workingDirTests").hide("fast");
            }
        }
        );
    
        $("#legacyWorkingDir-link").click(function(){
            if ($("#legacyWorkingDir").css("display")=="none"){
                $("#legacyWorkingDir").show("fast");
            }
            else {
                $("#legacyWorkingDir").hide("fast");
            }
        }
        );
    
        $("#inheritedWorkingdir-link").click(function(){
            if ($("#inheritedWorkingdir").css("display")=="none"){
                $("#inheritedWorkingdir").show("fast");
            }
            else {
                $("#inheritedWorkingdir").hide("fast");
            }
        }
        );
    
</script>
